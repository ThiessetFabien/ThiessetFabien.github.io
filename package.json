{
  "name": "portfolio",
  "description": "My personal portfolio crafted with ❤️ using Next.js",
  "author": {
    "name": "Thiesset Fabien",
    "email": "thiessetfabienpro@gmail.com"
  },
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "copy-leaflet-images": "mkdir -p public/images/leaflet && cp -r node_modules/leaflet/dist/images/* public/images/leaflet/",
    "dev": "dotenv -e .env.development -- sh -c 'pnpm run copy-leaflet-images && next dev'",
    "dev:prod": "dotenv -e .env.production -- sh -c 'pnpm run copy-leaflet-images && next dev'",
    "dev:analyze": "ANALYZE=true next dev",
    "build:analyze": "ANALYZE=true next build",
    "build": "dotenv -e .env.production -- sh -c 'pnpm run copy-leaflet-images && next build'",
    "prod": "dotenv -e .env.production -- npx netlify-cli deploy --prod",
    "deploy": "pnpm run build && pnpm run prod",
    "start": "dotenv -e .env.production -- sh -c 'next start'",
    "precommit": "npx lint-staged",
    "format": "prettier --write --ignore-path .gitignore .",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "clean:.next": "rm -rf .next",
    "clean:node_modules": "rm -rf node_modules",
    "clean:all": "rm -rf .next node_modules",
    "prepare": "husky install"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-hover-card": "^1.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-toggle": "^1.1.2",
    "@types/dompurify": "^3.2.0",
    "@types/react-responsive": "^8.0.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.25",
    "dompurify": "^3.2.4",
    "dotenv": "^16.4.7",
    "embla-carousel": "^8.5.2",
    "embla-carousel-autoplay": "^8.5.2",
    "embla-carousel-react": "^8.5.2",
    "framer-motion": "^12.5.0",
    "google-auth-library": "^9.15.1",
    "googleapis": "^144.0.0",
    "isomorphic-dompurify": "^2.22.0",
    "jsdom": "^26.0.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.461.0",
    "next": "15.1.3",
    "next-themes": "^0.4.6",
    "nodemailer": "^6.10.0",
    "react": "19.0.0-rc.1",
    "react-dom": "19.0.0-rc.1",
    "react-hook-form": "^7.54.2",
    "react-leaflet": "5.0.0-rc.2",
    "react-responsive": "^10.0.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^5.10.1",
    "@tailwindcss/postcss": "^4.0.15",
    "@types/leaflet": "^1.9.17",
    "@types/node": "^20.17.26",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.27.0",
    "@typescript-eslint/parser": "^8.27.0",
    "autoprefixer": "^10.4.21",
    "css-loader": "6.8.1",
    "cssnano": "^7.0.6",
    "cssnano-preset-default": "^7.0.6",
    "dotenv-cli": "^7.4.4",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.15",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-prettier": "^5.2.4",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "postcss": "^8.5.3",
    "postcss-import": "^16.1.0",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "responsive-loader": "^3.1.2",
    "sharp": "^0.33.5",
    "source-map-loader": "^5.0.0",
    "style-loader": "3.3.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.2",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "pnpm": {
    "overrides": {},
    "ignoredBuiltDependencies": [
      "@parcel/watcher",
      "sharp"
    ],
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "sharp"
    ]
  }
}
