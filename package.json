{
  "name": "portfolio",
  "description": "My personal portfolio crafted with ❤️ using Next.js",
  "author": {
    "name": "Thiesset Fabien",
    "email": "thiessetfabienpro@gmail.com"
  },
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "scripts": {
    "typecheck": "tsc --noEmit --pretty",
    "prepare": "husky",
    "menu": "./scripts/menu.sh",
    "copy-leaflet-images": "mkdir -p public/images/leaflet && cp -r node_modules/leaflet/dist/images/* public/images/leaflet/",
    "dev": "dotenv -e .env.development -- sh -c 'bun run copy-leaflet-images && bun --bun next dev'",
    "dev:prod": "NODE_ENV=development dotenv -e .env.production -- sh -c 'bun run copy-leaflet-images && bun --bun next dev'",
    "dev:turbo": "./scripts/run-turbo.sh",
    "dev:turbo:legacy": "dotenv -e .env.development -- sh -c 'bun run copy-leaflet-images && bun --bun next dev --turbo'",
    "dev:standard": "./scripts/run-standard.sh",
    "restore-swc": "./scripts/restore-swc.sh",
    "dev:universal": "./scripts/dev.sh",
    "dev:fast": "./scripts/dev.sh --mode=fast",
    "dev:node": "./scripts/dev.sh --runtime=node",
    "dev:analyze": "ANALYZE=true bun --bun next dev",
    "dev:optimized": "./scripts/dev.sh --mode=optimized",
    "build": "NODE_ENV=production dotenv -e .env.production -- sh -c 'bun run copy-leaflet-images && NODE_OPTIONS=--max-old-space-size=4096 bun --bun next build'",
    "build:analyze": "ANALYZE=true bun --bun next build",
    "build:fast": "./scripts/build-unified.sh --fast",
    "build:optimized": "./scripts/build-unified.sh --optimize",
    "start": "dotenv -e .env.production -- sh -c 'bun --bun next start'",
    "prod": "dotenv -e .env.production -- bunx netlify-cli deploy --prod",
    "deploy": "bun run build && bun run prod",
    "deploy:full": "./scripts/pre-deploy-check.sh && bun run build:optimized && bun run prod",
    "lint": "bun --bun next lint",
    "lint:fix": "bun --bun next lint --fix",
    "lint:strict": "bun --bun next lint --max-warnings=0",
    "format": "prettier --write --ignore-path .gitignore .",
    "format:check": "prettier --check --ignore-path .gitignore .",
    "type-check": "tsc --noEmit",
    "fix": "./scripts/fix.sh",
    "fix:lint": "./scripts/fix.sh --lint",
    "fix:format": "./scripts/fix.sh --format",
    "fix:ts": "./scripts/fix.sh --ts",
    "fix:react": "./scripts/fix.sh --react",
    "fix:bun": "./scripts/fix.sh --bun",
    "fix:npm": "./scripts/fix.sh --npm",
    "fix:code": "./scripts/fix.sh --bun",
    "clean:.next": "rm -rf .next",
    "clean:node_modules": "rm -rf node_modules",
    "clean:all": "rm -rf .next node_modules",
    "clean:empty": "bun run scripts/remove-empty-dirs.js",
    "clean:temp": "./scripts/cleanup.sh",
    "clean:deps": "./scripts/clean-unused-deps.sh",
    "test:leaflet": "./scripts/test-leaflet-component.sh",
    "clean:backups": "rm -rf .backups",
    "reset": "./scripts/reset-env.sh",
    "install:bun": "./scripts/install-bun.sh",
    "install:deps": "./scripts/install-deps.sh",
    "install:netlify": "./scripts/install-netlify.sh",
    "optimize": "./scripts/optimize.sh",
    "optimize:cache": "./scripts/optimize.sh --cache",
    "optimize:images": "./scripts/optimize.sh --images",
    "optimize:responsive": "./scripts/optimize.sh --responsive",
    "preheat-cache": "./scripts/preheat-cache.sh",
    "check": "./scripts/check.sh",
    "check:health": "./scripts/check.sh --health",
    "check:code": "./scripts/check.sh --code",
    "check:perf": "./scripts/check.sh --perf",
    "check:config": "./scripts/check.sh --config",
    "check:lint": "./scripts/check.sh --lint",
    "check:swc": "./scripts/validate-swc-config.sh",
    "analyze:scripts": "./scripts/analyze-scripts.sh",
    "precommit": "bunx lint-staged",
    "toggle-runtime": "./scripts/toggle-runtime.sh",
    "quality": "./scripts/maintenance-qualite.sh",
    "ci:check": "bun run type-check && bun run format:check && bun run lint:strict",
    "help": "./scripts/help.sh"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-hover-card": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-toggle": "^1.1.9",
    "@types/react-responsive": "^8.0.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "0.0.20",
    "dotenv": "^16.5.0",
    "embla-carousel": "^8.6.0",
    "embla-carousel-auto-scroll": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.12.2",
    "google-auth-library": "^9.15.1",
    "googleapis": "^144.0.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.461.0",
    "next": "15.1.3",
    "next-themes": "^0.4.6",
    "qr-code-styling": "^1.9.2",
    "react": "19.0.0-rc.1",
    "react-dom": "19.0.0-rc.1",
    "react-hook-form": "^7.56.4",
    "react-intersection-observer": "^9.16.0",
    "react-leaflet": "5.0.0-rc.2",
    "react-responsive": "^10.0.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.23"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.18",
    "@types/node": "^20.17.50",
    "@types/react": "^18.3.22",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "autoprefixer": "^10.4.21",
    "css-loader": "6.8.1",
    "cssnano": "^7.0.7",
    "cssnano-preset-default": "^7.0.7",
    "dotenv-cli": "^7.4.4",
    "esbuild": "^0.22.0",
    "eslint": "8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-next": "14.2.15",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-import-alias": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.0.0",
    "postcss": "^8.5.3",
    "postcss-import": "^16.1.0",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "responsive-loader": "^3.1.2",
    "sharp": "^0.34.2",
    "style-loader": "3.3.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "bun": {
    "workspaces": [],
    "dependencies": {
      "ignore": [
        "@parcel/watcher",
        "sharp"
      ],
      "nativeNodeModules": [
        "@parcel/watcher",
        "sharp"
      ]
    },
    "peerDependenciesMeta": {
      "react": {
        "optional": true
      },
      "react-dom": {
        "optional": true
      }
    }
  }
}
